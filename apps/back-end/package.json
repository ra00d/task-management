{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "env-cmd typeorm-ts-node-commonjs",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:css": "tailwindcss -i ./public/css/input.css -o ./public/css/app.css --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "postinstall": "hygen init self",
    "make:resource": "hygen resource new",
    "make:migration": "hygen migration new",
    "migrate": "pnpm run typeorm migration:generate  --d src/database/data-source.ts ",
    "migration:run": "pnpm run typeorm migration:run  --d src/database/data-source.ts ",
    "migration:show": "pnpm run typeorm migration:show  --d src/database/data-source.ts ",
    "make:seed": "env-cmd ts-node-esm ./node_modules/typeorm-extension/bin/cli.mjs seed:create -r src/database/seeds --name ",
    "db:seed": "env-cmd ts-node-esm ./node_modules/typeorm-extension/bin/cli.mjs seed:run -d src/database/data-source.ts ",
    "db:seeds": "env-cmd ts-node ./src/scripts/seed.ts",
    "db:create": "env-cmd ts-node-esm ./node_modules/typeorm-extension/bin/cli.mjs db:create -d src/database/data-source.ts ",
    "db:sync": "pnpm run typeorm schema:sync  --d src/database/data-source.ts ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@faire/mjml-react": "^3.3.0",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "@squareboat/nest-mailman": "^1.0.6",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.11",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "csrf-sync": "^4.0.3",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "hbs": "^4.2.0",
    "helmet": "^7.1.0",
    "mysql2": "^3.9.7",
    "nestjs-i18n": "^10.4.5",
    "nunjucks": "^3.2.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@tailwindcss/typography": "^0.5.13",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/hbs": "^4.0.4",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/nunjucks": "^3.2.6",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.2",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "daisyui": "^4.11.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "hygen": "^6.2.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
